{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended",
        "tslint-react"
    ],
    "rules": {
        "max-line-length": {
            "options": [ 160 ]
        },
        "one-line": {
            "options": [ "check-open-brace", "check-whitespace" ]
        },
        "object-literal-sort-keys": {
            "severity": "off"
        },
        "arrow-parens": {
            "severity": "off"
        }
    }
}