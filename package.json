{
  "name": "frui.ts",
  "publishConfig": {
    "registry": "https://nexus.eman.cz/repository/eman-npm/"
  },
  "version": "0.1.0",
  "description": "MVVM UI framework",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "scripts": {
    "version": "lerna version -y --no-push --no-git-tag-version",
    "publish:storybook": "build-storybook -c .storybook -o dist/storybook",
    "publish:docs": "typedoc packages --out dist/docs --hideGenerator --exclude '**/node_modules' --ignoreCompilerErrors",
    "publish": "rimraf dist && yarn run publish:storybook && yarn run publish:docs",
    "cleanbuild": "lerna run clean && lerna run build",
    "build": "lerna run build",
    "test": "jest",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "lint": "eslint ./{packages,examples}/**/*.{ts,tsx}",
    "validate": "yarn run lint && yarn run cleanbuild && yarn run test && yarn run publish"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.eman.cz:frui.ts/frui.ts-fe.git"
  },
  "keywords": [
    "mvvm",
    "react",
    "mobx"
  ],
  "author": "Augustin Å ulc",
  "license": "MIT",
  "devDependencies": {
    "@storybook/addon-actions": "^5.3.6",
    "@storybook/addon-console": "^1.2.1",
    "@storybook/addon-info": "^5.3.6",
    "@storybook/addon-storysource": "^5.3.6",
    "@storybook/react": "^5.3.6",
    "@types/html-webpack-plugin": "^3.2.1",
    "@types/jest": "^24.9.0",
    "@types/lodash": "^4.14.149",
    "@types/react": "^16.9.17",
    "@types/react-dom": "^16.9.4",
    "@types/reflect-metadata": "^0.1.0",
    "@types/typescript": "^2.0.0",
    "@types/webpack-dev-server": "^3.9.0",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.18.0",
    "fetch-with-proxy": "^1.1.0",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-mock-extended": "^1.0.7",
    "lerna": "^3.20.2",
    "local-cors-proxy": "^1.0.2",
    "react-docgen-typescript-loader": "^3.6.0",
    "ts-jest": "^24.3.0",
    "ts-loader": "^6.2.1",
    "tslint": "^5.14.0",
    "tslint-sonarts": "^1.9.0",
    "typedoc": "^0.16.11",
    "typedoc-plugin-no-inherit": "^1.1.10",
    "typescript": "^3.7.5",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.10.1"
  },
  "dependencies": {
    "bind-decorator": "^1.0.11",
    "lodash": "^4.17.15",
    "mobx": "^4.15.2",
    "mobx-react-lite": "^1.5.2",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "reflect-metadata": "^0.1.13"
  }
}
