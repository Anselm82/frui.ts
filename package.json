{
  "name": "frui.ts",
  "version": "0.1.0",
  "description": "MVVM UI framework",
  "main": "index.js",
  "directories": {
    "example": "examples",
    "test": "tests"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "npm run build:library && npm run build:storybook",
    "build:library": "webpack",
    "build:storybook": "build-storybook -c .storybook -o dist/storybook",
    "test": "jest",
    "storybook": "start-storybook -p 9001 -c .storybook",
    "validate": "tsc --noEmit && npm run lint",
    "lint": "tslint --project ."
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.eman.cz:frui.ts/frui.ts-fe.git"
  },
  "keywords": [
    "mvvm",
    "react",
    "mobx"
  ],
  "author": "Augustin Å ulc",
  "license": "ISC",
  "devDependencies": {
    "@storybook/addon-actions": "^4.1.11",
    "@storybook/addon-info": "^4.1.11",
    "@storybook/addon-storysource": "^4.1.11",
    "@storybook/react": "^4.1.11",
    "@types/jest": "^24.0.0",
    "@types/react": "^16.8.2",
    "@types/react-dom": "^16.8.0",
    "@types/storybook__addon-actions": "^3.4.1",
    "@types/storybook__react": "^4.0.0",
    "@types/typescript": "^2.0.0",
    "jest": "^24.1.0",
    "react-docgen-typescript-loader": "^3.0.1",
    "ts-jest": "^23.10.5",
    "ts-loader": "^5.3.3",
    "tslint": "^5.12.1",
    "tslint-react": "^3.6.0",
    "typescript": "^3.3.3",
    "webpack-cli": "^3.2.3"
  },
  "dependencies": {
    "bind-decorator": "^1.0.11",
    "mobx": "^5.9.0",
    "mobx-react-lite": "^1.0.0",
    "react": "^16.8.1",
    "react-dom": "^16.8.1"
  }
}
